{
  "extends": "@logux/stylelint-config",
  "ignoreFiles": [
    "web/dist/**/*",
    "storybook-static/**/*"
  ],
  "rules": {
    "function-disallowed-list": ["rgb", "rgba", "hsl", "hsla"]
  },
  "overrides": [
    {
        "files": ["**/*.svelte"],
        "customSyntax": "postcss-html"
    }
  ]
}
